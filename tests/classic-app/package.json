{
  "name": "classic-app",
  "private": true,
  "description": "Enabling better Test selectors in Ember.js applications.",
  "license": "MIT",
  "author": "Mainmatter GmbH",
  "repository": "https://github.com/mainmatter/ember-test-selectors",
  "scripts": {
    "build": "ember build",
    "lint": "eslint . --cache",
    "start": "ember server",
    "test": "npm run test:keep && npm run test:strip",
    "test:all": "ember try:each",
    "test:keep": "ember test",
    "test:strip": "STRIP_TEST_SELECTORS=true ember test"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@ember/optional-features": "2.2.0",
    "@ember/test-helpers": "2.9.4",
    "@embroider/test-setup": "1.8.3",
    "broccoli-stew": "3.0.0",
    "calculate-cache-key-for-tree": "^2.0.0",
    "ember-auto-import": "2.10.0",
    "ember-cli": "4.12.3",
    "ember-cli-babel": "^7.26.4",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-version-checker": "^5.1.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-qunit": "8.1.1",
    "ember-resolver": "8.1.0",
    "ember-source": "4.12.4",
    "ember-source-channel-url": "3.0.0",
    "ember-test-selectors": "workspace:*",
    "ember-try": "3.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-simplabs": "0.4.0",
    "eslint-plugin-ember": "12.3.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "8.1.2",
    "loader.js": "4.7.0",
    "prettier": "2.8.8",
    "qunit": "2.19.4",
    "qunit-dom": "3.4.0",
    "webpack": "5.97.1"
  },
  "engines": {
    "node": "18.* || 20.* || >= 22.*"
  },
  "ember": {
    "edition": "octane"
  }
}
