{
  "name": "ember-test-selectors",
  "version": "7.1.0",
  "description": "Enabling better Test selectors in Ember.js applications.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Mainmatter GmbH",
  "repository": "https://github.com/mainmatter/ember-test-selectors",
  "scripts": {
    "lint": "eslint . --cache"
  },
  "dependencies": {
    "calculate-cache-key-for-tree": "^2.0.0",
    "ember-cli-babel": "^7.26.4",
    "ember-cli-version-checker": "^5.1.2",
    "strip-test-selectors": "workspace:*"
  },
  "devDependencies": {
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-config-simplabs": "0.4.0",
    "eslint-plugin-ember": "12.5.0",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-qunit": "8.1.2",
    "prettier": "3.6.1"
  },
  "engines": {
    "node": "18.* || 20.* || >= 22.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "before": [
      "ember-cli-htmlbars",
      "ember-cli-htmlbars-inline-precompile"
    ],
    "configPath": "tests/dummy/config"
  }
}
